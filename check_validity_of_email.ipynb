      "source": [
        "\n",
        "import re\n",
        "s=input(\"Enter your Email Address\")\n",
        "x=(re.search('[a-zA-Z]+[.]*[_0-9a-zA-Z]*@[a-zA-Z]+.[a-zA-z]+',s))\n",
        "print(x)\n",
        "if x:\n",
        "    print(\"your Email is vaild\")\n",
        "else:\n",
        "    print(\"invalid email\")\n",
        "\n"
      ],
